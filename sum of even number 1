import java.Scanner;
public class Main
{
public static void Main(string[]args)
{
Scanner Sc = new Scanner (system.in);
int n = Sc.nextint();
int num = 2;
for(int i=1;i<=n;i++);
{
for(int j=1;j<=n;j++);
{
for(int k=1;k<=n;k++);
{
if(num%2==0);
{
system.out.print(num+ "");
}
system.out.println();
scanner.close();
}
